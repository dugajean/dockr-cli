{
    "name": "dugajean/dockr-cli",
    "type": "library",
    "description": "Easy Docker Compose setup for your LAMP and LEMP projects.",
    "keywords": ["docker", "container", "dockr", "docker-compose", "cli"],
    "homepage": "http://dukagj.in",
    "license": "MIT",
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "me@dukagj.in"
        }
    ],
    "bin": [
        "bin/dockr"
    ],
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "ext-ctype": "*",
        "symfony/console": "^4.2",
        "symfony/finder": "^4.2",
        "dugajean/pouch": "^2",
        "symfony/event-dispatcher": "^4.2",
        "padraic/phar-updater": "^1.0",
        "symfony/process": "^4.2",
        "symfony/dotenv": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.13",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Dockr\\": "src/"
        },
        "files": [
            "src/Helpers/utils.php",
            "config/pouch.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dockr\\Tests\\": "tests/PHPUnit"
        }
    }
}
