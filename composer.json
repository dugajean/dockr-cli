{
    "name": "dugajean/dockr-cli",
    "type": "library",
    "description": "Easy Docker Compose setup for your LAMP and LEMP projects.",
    "keywords": ["docker", "container", "dockr", "docker-compose", "cli"],
    "homepage": "http://dukagj.in",
    "license": "MIT",
    "authors": [
        {
            "name": "Dukagjin Surdulli",
            "email": "me@dukagj.in"
        }
    ],
    "require": {
        "php": "^7",
        "ext-json": "*",
        "ext-ctype": "*",
        "symfony/console": "^4.2",
        "symfony/finder": "^4.2",
        "dugajean/pouch": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.13",
        "mikey179/vfsStream": "^1.6",
        "brain/monkey": "^2.2",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dockr\\": "src/"
        },
        "files": [
            "src/Helpers/utils.php",
            "config/pouch.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dockr\\Tests\\": "tests/"
        }
    }
}
